<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    <servlet-name>PathDemo</servlet-name>
    <servlet-class>com.wzd.path.PathDemo</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ResourceDemo</servlet-name>
    <servlet-class>com.wzd.resource.ResourceDemo</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CookieDemo</servlet-name>
    <servlet-class>com.wzd.cookie.CookieDemo</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteCookieDemo</servlet-name>
    <servlet-class>com.wzd.cookie.DeleteCookieDemo</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LastTime</servlet-name>
    <servlet-class>com.wzd.cookie.LastTime</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DetailServlet</servlet-name>
    <servlet-class>com.wzd.cart.servlet.DetailServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ListServlet</servlet-name>
    <servlet-class>com.wzd.cart.servlet.ListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Session1</servlet-name>
    <servlet-class>com.wzd.session.Session1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Session2</servlet-name>
    <servlet-class>com.wzd.session.Session2</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>com.wzd.logindemo.IndexServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.wzd.logindemo.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogOutServlet</servlet-name>
    <servlet-class>com.wzd.logindemo.LogOutServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>PathDemo</servlet-name>
    <url-pattern>/path</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ResourceDemo</servlet-name>
    <url-pattern>/resource</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CookieDemo</servlet-name>
    <url-pattern>/cookie</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteCookieDemo</servlet-name>
    <url-pattern>/deleteCookie</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LastTime</servlet-name>
    <url-pattern>/lastTime</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DetailServlet</servlet-name>
    <url-pattern>/DetailServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ListServlet</servlet-name>
    <url-pattern>/ListServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Session1</servlet-name>
    <url-pattern>/session1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Session2</servlet-name>
    <url-pattern>/session2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogOutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
</web-app>
